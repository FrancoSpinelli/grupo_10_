<!DOCTYPE html>
<html lang="en">

<%- include('../partials/head.ejs')%>

  <body>

    <%- include('../partials/header.ejs')%>

      <main>

        <article class="carrusel"></article>

        <h2 class="productosDestacados">PRODUCTOS PARA PECES</h2>

        <article class="productosDestacados">

          <% productsPeces.forEach( ( producto )=> { %>

            <div class="productos">

              <h3><%= producto.name %></h3>

              <img src="/img/products/<%= producto.image%>" alt="alimento" />

              <p><%= producto.description %></p>

              <p class="precioshome"><%= producto.price%></p>

              <div class="homeagregaralcarrito">
                <button class="botonagregaralcarrito">AGREGAR</button>
              </div>

            </div>

          <% }); %>

        </article>

      </main>

    <%- include('../partials/footer.ejs')%>

  </body>

</html>